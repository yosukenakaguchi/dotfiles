{
  "editor.tabSize": 2,
  "editor.cursorStyle": "block",
  "editor.minimap.enabled": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": false,
  "editor.formatOnType": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": [
    "source.organizeImports",
    "source.fixAll.eslint"
  ],
  "shellcheck.executablePath": "/Users/ynakaguchi/dotfiles/scripts/shellcheck.sh",
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "settingsSync.ignoredSettings": ["-shellcheck.executablePath"],
  "[properties]": {
    "editor.formatOnSave": false
  },
  "ruby.intellisense": "rubyLocate",
  "ruby.codeCompletion": "rcodetools",
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Tokyo Night",
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "vscode-neovim.revealCursorScrollLine": false,
  "vscode-neovim.neovimInitVimPaths.darwin": "/Users/ynakaguchi/.config/nvim/init.vim",
  "workbench.view.alwaysShowHeaderActions": true,
  "shellcheck.ignoreFileSchemes": ["git", "gitfs", "json"],
  "diffEditor.ignoreTrimWhitespace": false,
  "git.mergeEditor": false,
  "window.zoomLevel": 2
}
